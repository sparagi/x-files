	resultTable do: [:resultArray | | className |
		className := resultArray at: 1.

		{	{'ordered' . resultArray at: 2}.
			{'bounded' . resultArray at: 3}.
			{'cardinality'. resultArray at: 4}.
			{'numeric'. resultArray at: 5} } do: [:messageAndAnswer | | message answer method |
				message := messageAndAnswer at: 1.
				answer := messageAndAnswer at: 2. 
				method := String streamContents: [:stream |
					stream
						nextPutAll: 'test', message capitalized, className;
						cr;
						nextPutAll: '	"From table C.1 Fundamental Facets in XML Schema Part 2: Datatypes Second Edition."';
						cr;
						nextPutAll: '	"Automatically generated by class>>generateFundamentalFacetTests; do not edit here."';
						cr;
						cr;
						nextPutAll: '	self should: [', className, ' ', message, ' = ', answer, ']';
						cr].
				self compile: method classified: 'fundamental facets']]